<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_1.xsd"
             version="3.1">
	<persistence-unit name="Database">
		<class>entity.ContactOpportunity</class>
		<class>entity.Demonstration</class>
		<class>entity.DemonstrationFile</class>
		<class>entity.Developer</class>
		<class>entity.Project</class>
		<properties>
			<property name="jakarta.persistence.jdbc.driver" value="org.firebirdsql.jdbc.FBDriver"/>
		</properties>
	</persistence-unit>
</persistence>
